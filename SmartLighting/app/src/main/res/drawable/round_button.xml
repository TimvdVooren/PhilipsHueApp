<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android" >
    <item android:state_pressed="true" >
        <shape android:shape="rectangle"  >
            <corners android:radius="3dip" />
            <stroke android:width="1dip" android:color="#93ff54" />
            <gradient android:angle="-90" android:startColor="#b6b6b6" android:endColor="#6b6b6b"  />
        </shape>
    </item>
    <item android:state_focused="true">
        <shape android:shape="rectangle"  >
            <corners android:radius="3dip" />
            <stroke android:width="1dip" android:color="#93ff54" />
            <solid android:color="#b6b6b6"/>
        </shape>
    </item>
    <item >
        <shape android:shape="rectangle"  >
            <corners android:radius="3dip" />
            <stroke android:width="1dip" android:color="#93ff54" />
            <gradient android:angle="-90" android:startColor="#6b6b6b" android:endColor="#b6b6b6" />
        </shape>
    </item>
</selector>